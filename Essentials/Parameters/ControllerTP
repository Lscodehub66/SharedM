// let LoadCode = Load("ControllerTP","Code"), EVA = Expression.Evaluate(LoadCode, #shared) in EVA
let
    Source = Load("Controller"),
    #"Removed Other Columns" = List.Accumulate(
        {"Monday", "当前", "上周", "年初", "22年初", "近1年", "近5年", "wFilter_PreviousMon_Date"},
        Source,
        (x, y) => try Table.TransformColumns(x, {{y, each Date.ToText(_, "yyyyMMdd")}}) otherwise y
    ),
    #"Demoted Headers" = Table.DemoteHeaders(#"Removed Other Columns"),
    #"Transposed Table" = Table.Transpose(#"Demoted Headers")
in
    #"Transposed Table"
