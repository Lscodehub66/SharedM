// let LoadCode = Load("ForReview_wCredit","Code"), EVA = Expression.Evaluate(LoadCode ,#shared) in EVA

let
    Source = try CWB("DLD_Conso") otherwise Load("DLD_Conso"),
    SCol = Table.SelectColumns( Source, {"信用主体","债券简称"}),
     FromForReviewIssuer = Table.SelectColumns(Table.SelectRows(Load("ForReview_Issuer"), each Text.Contains([信用主体], "【")), {"信用主体"}),
    Combined = fx("Combine")({ SCol ,FromForReviewIssuer}, {"信用主体"}),
    TFCol = Table.TransformColumns(
        Combined,
        {
            {
                "信用主体",
                each
                    if Text.Contains(_, "【") then
                        Text.BetweenDelimiters(Text.BeforeDelimiter(_, "【", {0, RelativePosition.FromEnd}), "【", "】")
                    else
                        _
            }
        }
    ),
    Replaced = Table.ReplaceValue(TFCol, null, each [信用主体], Replacer.ReplaceValue, {"债券简称"}),
    MergedQ = Table.NestedJoin(Replaced, {"信用主体"}, fx("Source")("DEALSMASTER_mCredit"), {"信用主体"}, Prefix & "Data", JoinKind.LeftAnti),
    Function = Table.AddColumn(MergedQ, "SHORTNAMER", each fx("SNBankCorp")([债券简称])),
    RemovedCol = Table.SelectColumns(Function, {"信用主体", "SHORTNAMER", "中资属性", "板块"}, MissingField.UseNull),
    BlankToInput = Table.ReplaceValue(RemovedCol, null, "【】", Replacer.ReplaceValue, {"中资属性", "板块"})
in
    BlankToInput