let
    DBName = "mBond",
    Source = {"Index"} & List.Intersect( {Get("Header_ForReviewHD"),CWB(    DBName ,"CN")}),
    Custom1 = Table.SelectColumns(  Load("DLD_Filtered"), Source, MissingField.Ignore),
        Custom2 = fx("SelfJoin_R")(   Custom1 , CWB("ForReview_wBond"), {"ISIN"})
   
in
    Custom2