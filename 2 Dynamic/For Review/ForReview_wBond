// let LoadCode = Load("ForReview_wBond","Code"), EVA = Expression.Evaluate(LoadCode ,#shared) in EVA
let
    _input = "wBond",
    _PK = "ISIN",
    Name = "m" & Text.AfterDelimiter(_input, "w"),
    HD = fx("ForReviewHD")(_input),
    Consotbl = Load("DLD_Filtered"),
    Filtered = Table.SelectRows(Consotbl, each not List.Contains({"n/a"}, Record.Field(_, Name))),
    Custom2 = fx("SelfJoin_R")(Filtered, CWB("ForReview_" & _input), {_PK}),
    SCol = Table.SelectColumns(Custom2, HD, MissingField.UseNull),
    Sorted = fx("SortNIndex")(SCol)
in
    Sorted
