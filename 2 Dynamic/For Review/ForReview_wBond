let
    Name = "wIssue",
    Consotbl = Load("DLD_Filtered"),
    HD = fx("ForReviewHD")(Name),
    Custom1 = Table.SelectColumns(Consotbl, HD, MissingField.UseNull),
    Custom2 = fx("SelfJoin_R")(Custom1, CWB("ForReview_wBond"), {"ISIN"})
in
    Custom2
