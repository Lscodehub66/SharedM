let
    Source = CWB("mChart"),
    Header7 = Get("HeaderChart"),
    MergedDB = Load("fxDBMerger")(Source),
    FinalizedDB = Load("fxDBFinalization")(MergedDB),
    SCol = Table.SelectColumns(FinalizedDB, Header7, MissingField.UseNull)
in
    SCol