
// Indice_1Y
let

 Source      = ConnectDB{[Item = "mIndice"]}[Data],
 SelectedCol = Table.SelectColumns ( Source, {"Date"}& Table.SelectRows(Dimtbl_Indice, each ([IS_1YR] = "Y"))[PK]), 
 Filtered    = Table.SelectRows ( SelectedCol, each [Date] >= Status[近1年]{0} ), 
 Sorted      = Load("fxSortNIndex")(Filtered,  "Date",null,null,null,"N")
in
 Sorted