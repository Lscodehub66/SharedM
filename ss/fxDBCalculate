(TableName as table) =>
    let
        //
        Source = Table.Buffer(fx("Data")(TableName)),
        //
        #"Added IsDebut" = fx("AddIsDebut")(Source),
        #"Replaced NullAmount" = fx("ReplaceNullOutstanding")(#"Added IsDebut"),
        #"Added FolderPath" = fx("FolderPath")(#"Replaced NullAmount"),
        #"Added 清算地" = fx("AddClearing")(#"Added FolderPath"),
        #"Added BBGFX" = fx("AddBBGFX")(#"Added 清算地"),
        #"Added BOCOMDEAL?" = fx("fxAddBOCOMDEAL")(#"Added BBGFX"),
        #"Added 美元规模分类" = fx("GroupSize")(#"Added BOCOMDEAL?"),
        #"Added 年期结构货币分类" = fx("GpTenorSTRCCY")(#"Added 美元规模分类"),
        #"Added Listing" = fx("GpListing")(#"Added 年期结构货币分类"),
        #"Added AllRtg" = fx("GpRtgIsIG")(#"Added Listing"),
        //
        Written_Stats = fx("WriteStats")(#"Added AllRtg"),
        Written_Tenor = fx("WriteTenor")(Written_Stats),
        Written_CCYSize = fx("WriteCCYSize")(Written_Tenor),
        Written_Theme = fx("WriteTheme")(Written_CCYSize),
        Written_Role = fx("WriteRole")(Written_Theme),
        CombinedName = fx("NameCombiner")(Written_Role),
        //
        Checked = fx("CheckNDupe")(CombinedName),
        Sorted = fx("SortNIndex")(Checked),
        Presented = fx("PresentNoData")(Sorted)
    in
        Presented
