let
 Source = ( TextName as text ) =>
  let
   Output = fxSmartClean ( TextName, "SBLC" ), 
   Text1  = Text.BeforeDelimiter ( Output, "银行" ), 
   Text2  = 
    if Text1 = "" then
     "--"
    else if Text.Contains ( Text1, "(中国)" ) then
     Text1
    else if Text.EndsWith ( Text1, "农商" ) then
     Text1 & "行"
    else
     Text1 & "银行"
  in
   Text2
in
 Source