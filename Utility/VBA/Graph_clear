Private Sub Worksheet_Change(ByVal Target As Range)
    Dim KeyCells As Range

' The variable KeyCells contains the cells that will
    ' cause an alert when they are changed.
    Set KeyCells = Range("I4")

If Not Application.Intersect(KeyCells, Range(Target.Address)) _
           Is Nothing Then

' Display a message when one of the designated cells has been
        ' changed.
        ' Place your code here.
                
        Sheet26.Activate
        Application.GoTo Reference:="wEcon_Input_ToClear"
        Selection.ClearContents
        Application.GoTo Reference:="wEcon"
        Selection.ClearContents
        Application.GoTo Reference:="wFuture_Input_ToClear"
        Selection.ClearContents
        Application.GoTo Reference:="wFuture"
        Selection.ClearContents
    
        
        Sheet32.Activate
        Application.GoTo Reference:="wNIMSum_Load_Clear"
        Selection.ClearContents
       
        
         Sheet26.Select
        Range("J4").Select

End If
End Sub



