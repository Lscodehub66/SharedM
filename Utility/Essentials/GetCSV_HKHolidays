let
    fxJSON_TF = Load("fxJSON_TF"),
    //
    GPS = try Excel.CurrentWorkbook(){[Name = "GPS"]}[Content][Column1]{0} otherwise "Global",
    Source =
        if GPS = "Remote" then
            Folder.Files("C:\Users\ktkt2\OneDrive\EXE Team\DataHub\SharedM\Utility\json")
        else
            Folder.Files("\\itd-m09-fs02\dcm$\Team Folder\Execution Team\DataHub\SharedM\Utility\json"),
    // En
    #"Filtered Rows" = Table.SelectRows(Source, each ([Extension] = ".json")),
    #"Filtered Rows1" = Table.SelectRows(#"Filtered Rows", each Text.Contains([Name], "-en")),
    #"Invoked Custom Function" = Table.AddColumn(
        #"Filtered Rows1", "Transform File", each Load("fxJSON_TF")([Content])
    ),
    #"Removed Other Columns" = Table.SelectColumns(#"Invoked Custom Function", {"Transform File"}),
    #"Expanded Transform File" = Table.ExpandTableColumn(
        #"Removed Other Columns", "Transform File", {"Date", "summary"}, {"Date", "summary"}
    ),
    // sc
    #"sc_Filtered Rows" = Table.SelectRows(Source, each ([Extension] = ".json")),
    #"sc_Filtered Rows1" = Table.SelectRows(#"sc_Filtered Rows", each Text.Contains([Name], "-sc")),
    #"sc_nvoked Custom Function" = Table.AddColumn(
        #"sc_Filtered Rows1", "Transform File", each Load("fxJSON_TF")([Content])
    ),
    #"sc_Removed Other Columns" = Table.SelectColumns(#"sc_nvoked Custom Function", {"Transform File"}),
    #"sc_Expanded Transform File" = Table.ExpandTableColumn(
        #"sc_Removed Other Columns", "Transform File", {"Date", "summary"}, {"Date", "summary"}
    ),
    // merge
    Custom1 = fx(
        "Merge"
    )(
        #"Expanded Transform File", #"sc_Expanded Transform File", "Date", "", {"summary"}, {"summary_sc"}
    ),
    #"Merged DimDate" = fx("MergeDimDate")(Custom1, "Date", {"DayShortName", "YYYY"}),
    #"Sorted Rows" = Table.Sort(#"Merged DimDate", {{"Date", Order.Descending}})
in
    #"Sorted Rows"
