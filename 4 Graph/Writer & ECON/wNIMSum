let
    Source = fx("NIM_Indice")({"BeforePreviousMon.Date", "Sunday.Date"}),
    #"Removed Other Columns" = Table.SelectColumns(Source, {"每周第一天", "V_NIM_7D", "CT_NIM_7D"}),
    #"Removed Duplicates" = Table.Distinct(#"Removed Other Columns"),
    #"Sorted Rows" = Table.Sort(#"Removed Duplicates", {{"每周第一天", Order.Ascending}}),
    #"Demoted Headers" = Table.DemoteHeaders(#"Sorted Rows"),
    #"Transposed Table" = Table.Buffer(Table.Transpose(#"Demoted Headers"))
in
    #"Transposed Table"
