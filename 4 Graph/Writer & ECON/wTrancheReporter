let
    Source2 = CWB("Writer_Dealtbl"),
   WriteNewIssue = Table.Buffer(fx("DealReporter")(Source2, "Custom")),
    #"Sorted Rows" = Table.Sort(WriteNewIssue,{{"定价日", Order.Descending}}),
        Self = fx("fxReporter_SectorReviewed")( #"Sorted Rows" ),
#"Changed Type" = Table.TransformColumnTypes(Self, {{"新发总结板块", type text}}),
    Grouped = Table.Group( #"Changed Type", {"新发总结板块"}, {{"Description", each Text.Combine([Writer], "#(lf)")}}),
    #"Added Col" = Table.AddColumn(
        Grouped,
        "ObjectName",
        each if [新发总结板块] = "城投板块" then "nimlgfvbox" else if [新发总结板块] = "投资级板块" then "nimigbox" else "nimothersbox",
        type text
    )
in
    #"Added Col"

