let

    Source = CWB("mIndice"),
    CN_Indice = List.Buffer(Table.SelectRows(Load("Dimtbl_Indice"), each ([IS_5Y] <> ""))[PK]),
    SelectedCol = Table.SelectColumns(    Source, {"Date"} & CN_Indice),
    Sorted = fx("SortNIndex")(SelectedCol, "Date", null, null, null, "Y")
in
    Sorted
