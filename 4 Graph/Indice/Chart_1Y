//  let LoadCode = Load("Chart_1Y","Code"), EVA = Expression.Evaluate(LoadCode, #shared) in EVA
let

    Source = fx("Excel")("DEALSMASTER_=MINDICE"),
    KeyDate = List.Buffer(Load("Controller")[近1年]){0},
    Filtered = Table.SelectRows(Source, each [Date] >= KeyDate),
    CN_Indice = List.Buffer(Table.SelectRows(Load("Dimtbl_Indice"), each ([IS_1Y] <> ""))[PK]),
    SelectedCol = Table.SelectColumns(Filtered, {"Date"} & CN_Indice),
    Sorted = fx("SortNIndex")(SelectedCol, "Date", null, null, null, "Y")
in
    Sorted
