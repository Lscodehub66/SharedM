// let LoadCode = Load("合规_23","Code"), EVA = Expression.Evaluate(LoadCode ,#shared) in EVA
let
    Source = fx("Source")("台账23_合规"),
    ATime = Table.AddColumn(Source, "Timestamp", each DateTime.FixedLocalNow()),
    CWB_tbl = CWB("CPL_23"),
    // if SourceQRC does not have this item then maintain current CWB data to append
    CWB_combined = try Load("fxCombine")({ATime, CWB_tbl}, {"序号"}) otherwise try CWB_tbl otherwise ATime,
    Output = try CWB_combined otherwise CWB_tbl,
    Filtered = Table.SelectRows( Output, each ([团队] <> null))
    
in
        Filtered 