let
    Source = CWB("mBISL"),
    mBISL = fx("mBISLCount")(Source),
    Source2 = CWB("mEXE"),
    Added0 = fx("AddSFCRole")(Source2),
    Added = fx("AddBOCOMOrder")(Added0),
    KeysTagged = Table.AddKey(Added, {"Check"}, true),
    mEXE_Joined = fx("Merge")(mBISL, KeysTagged, "Check", "Check", {"Y"}, {""})
in
    mEXE_Joined
