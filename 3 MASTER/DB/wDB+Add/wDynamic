let

    Source = fx("Excel")("DYNAMIC_wConso"),
    ACol = if not Table.HasColumns(Source, "mChart") then Table.AddColumn(Source, "mChart", each if [删除及隐藏] = "FIMTN" then "Y" else "", type text) else Source,
            ACol1 = if not Table.HasColumns(Source, "PRJ对账号") then Load("fxPRJSync")(   ACol, "台账序号", "定价日", "PRJ对账号") else ACol,
             #"Removed Columns" = Table.RemoveColumns(  ACol1,{"Updator", "Timestamp"},MissingField.Ignore) ,


        ManagedDB = fx("DBManager2")(#"Removed Columns")
in
    ManagedDB