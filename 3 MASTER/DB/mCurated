// let LoadCode = Load("mCurated","Code"), EVA = Expression.Evaluate(LoadCode ,#shared) in EVA
let
    AddCode = Load("fxAddToDB", "Code"),
    ManagerCode = Load("fxDBManager", "Code"),
    DB = "mCurated",
    DB_Source = CWB(DB),
    Parameter = Load("Controller")[RemoveFrom_DB]{0} ?? "",
    Added = fx("AddToDB")(DB, Load("wDynamicPQ"), Parameter),
    Revised =
        if Text.Contains(Parameter, "Stop") then
           try fx("DB_Revise")(DB_Source, CWB("ToRevise"), "Check") otherwise DB_Source
        else
          try  fx("DB_Revise")(Added, CWB("ToRevise"), "Check") otherwise Added,
    Managed = fx("DBManager")(Revised, "m")
in
    Managed
