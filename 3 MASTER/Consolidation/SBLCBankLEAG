// let LoadCode = Load("SBLCBankLEAG","Code"), EVA = Expression.Evaluate(LoadCode ,#shared)  in EVA
let
    MonitorName = "SBLC",
    Fn = fx("DBMonitor")(MonitorName),
    Grouped = Table.Group(
        Fn, {"备证行"}, {{"亿美元", each List.Sum([亿美元]), type number}, {"笔数", each Table.RowCount(_), Int64.Type}}
    ),
    Sorted = Table.Buffer(Table.Sort(Grouped, {{"笔数", Order.Descending}})),
    AddedIndex = Table.AddIndexColumn(Sorted, "笔数排名", 1, 1, Int64.Type),
    Filtered = Table.SelectRows(AddedIndex, each [笔数排名] <= 10),
    Reordered = Table.ReorderColumns(Filtered, List.Distinct({"笔数排名"} & Table.ColumnNames(Filtered))),
    CalculatedPt = Load("fxAddpt")(Reordered, "笔数")
in
    CalculatedPt
